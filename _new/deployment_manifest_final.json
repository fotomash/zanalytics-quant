{
  "version": "1.0.0",
  "name": "Zanalytics Pulse - Adaptive Wyckoff Integration",
  "status": "PRODUCTION_READY",
  "components": {
    "core": [
      "pulse_kernel.py",
      "confluence_scorer.py",
      "risk_enforcer.py"
    ],
    "adaptive": [
      "components/wyckoff_scorer.py",
      "components/wyckoff_adaptive.py",
      "components/wyckoff_mtf_resolver.py"
    ],
    "api": [
      "api/views_wyckoff.py",
      "api/urls_pulse.py"
    ],
    "services": [
      "services/tick_to_bar.py",
      "services/news_monitor.py"
    ],
    "utils": [
      "utils/metrics.py",
      "utils/wyckoff_calibration.py"
    ],
    "tests": [
      "tests/test_wyckoff_adaptive.py",
      "tests/test_wyckoff_mtf.py",
      "tests/test_news_buffer.py",
      "tests/test_pulse_integration.py"
    ]
  },
  "docker_services": [
    "pulse-kernel",
    "tick-to-bar",
    "news-monitor"
  ],
  "redis_streams": [
    "stream:pulse:wyckoff_adaptive:*",
    "stream:pulse:wyckoff_legacy:*",
    "stream:pulse:signals:*"
  ],
  "prometheus_metrics": [
    "wyckoff_phase_confidence",
    "wyckoff_conflicts_total",
    "wyckoff_events_confirmed_total",
    "wyckoff_news_mask_active",
    "pulse_confluence_score",
    "pulse_signals_generated_total",
    "pulse_risk_blocks_total"
  ],
  "deployment_steps": [
    "Deploy configuration files",
    "Update Django URLs",
    "Start Redis streams",
    "Deploy Docker services",
    "Verify health endpoints",
    "Enable shadow mode",
    "Monitor for 48 hours",
    "Review metrics",
    "Promote to production"
  ],
  "rollback_procedure": [
    "Disable adaptive features in config",
    "Revert to legacy Wyckoff scorer",
    "Clear Redis streams",
    "Restart services"
  ]
}