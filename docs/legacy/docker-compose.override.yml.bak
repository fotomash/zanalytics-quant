# docker-compose.override.yml
# Add this to your docker-compose to include the tick-to-bar service

version: '3.8'

services:
  tick-to-bar:
    build:
      context: .
      dockerfile: Dockerfile.ticktobar
    container_name: tick-to-bar
    environment:
      - REDIS_HOST=redis
      - REDIS_PORT=6379
      - SYMBOLS=EURUSD,GBPUSD,USDJPY,XAUUSD
      - LOG_LEVEL=INFO
    depends_on:
      - redis
      - mt5
    networks:
      - default
    restart: unless-stopped
    volumes:
      - ./tick_to_bar_service.py:
    command: python 
