# Sample environment configuration for zanalytics-quant
# Copy this file to .env and replace with real values.
# Never commit secrets or the .env file to version control.

# Secret key used by the MCP service for authentication
MCP_API_KEY=your_mcp_api_key_here

# Name of the Postgres database
POSTGRES_DB=your_postgres_db_here
# Username for the Postgres connection
POSTGRES_USER=your_postgres_user_here
# Password for the Postgres user
POSTGRES_PASSWORD=your_postgres_password_here
# Hostname of the Postgres service
POSTGRES_HOST=postgres
# Port where Postgres listens
POSTGRES_PORT=5432
# Complete Postgres DSN for async services
PG_DSN=postgresql://your_postgres_user_here:your_postgres_password_here@postgres:5432/your_postgres_db_here

# Redis connection URL
REDIS_URL=redis://redis:6379/0
